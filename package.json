{
  "name": "exprest",
  "version": "1.0.0",
  "private": true,
  "bin": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "test": "npm run lint",
    "lint": "eslint app/**/*.js",
    "format": "prettier-eslint --write app/**/*.js",
    "dist": "pkg . --out-path=dist/",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">=6"
  },
  "pkg": {
    "assets": [],
    "targets": [
      "win",
      "linux",
      "macos"
    ]
  },
  "lint-staged": {
    "app/**/*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "dependencies": {
    "compression": "^1.7.2",
    "cors": "^2.8.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "http-errors": "^1.6.3",
    "morgan": "^1.8.2"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "pkg": "^4.3.1",
    "prettier": "^1.11.1",
    "prettier-eslint-cli": "^4.7.1"
  }
}
